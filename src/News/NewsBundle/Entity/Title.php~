<?php

namespace News\NewsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;



/**
 * Title
 *
 * @ORM\Table(name="title")
 * @ORM\Entity;
 */
class Title {
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    /**
     *@ORM\Column(type="text", length=200)
     */
    private $title_txt;
    /**
     * @ORM\Column(type="datetime")
     */
    private $updated;


}
